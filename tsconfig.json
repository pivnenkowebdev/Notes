{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": [
    "src/**/*",
    "custom.d.ts"
  ],
  "exclude": [
    "node_modules",
    ".git",
    "webpack.config.js",
    "**/.eslintrc.js"
  ]
}